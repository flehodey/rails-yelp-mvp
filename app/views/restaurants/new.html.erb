<h2> Fill in this form to create a restaurant </h2>

<%= form_for(@restaurant) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :adress %>
  <%= f.text_field :adress %>

  <%= f.label :phone_number %>
  <%= f.text_field :phone_number %>

  <%= f.label :category %>
  <%= f.text_field :category %>


  <%= f.submit %>
<% end %>
